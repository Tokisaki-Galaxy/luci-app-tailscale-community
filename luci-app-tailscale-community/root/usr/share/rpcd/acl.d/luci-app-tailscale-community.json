{
	"unauthenticated": {
		"description": "unauthenticated user group",
		"read": {
			"ubus": {
				"tailscale": []
			}
		}
	},
	"user": {
		"description": "Allow user access to tailscale",
		"read": {
			"ubus": {
				"tailscale": [
					"do_login",
					"get_status",
					"get_settings",
					"get_subroutes"
				]
			},
			"uci": [ "tailscale" ]
		},
		"write": {
			"ubus": {
				"tailscale": [
					"do_logout",
					"set_settings"
				]
			},
			"uci": [ "tailscale" ]
		}
	}
}
